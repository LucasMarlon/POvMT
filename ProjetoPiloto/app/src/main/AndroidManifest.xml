<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="povmt.projeto.les.povmt.projetopiloto" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".views.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".views.AtividadeActivity" >
        </activity>
        <activity
            android:name=".views.NovaAtividadeActivity"
            android:label="@string/title_activity_new_activity" >
        </activity>
        <activity
            android:name=".views.AcompanhamentoActivity"
            android:label="@string/title_activity_acompanhamento" >
        </activity>
        <!--
             ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".views.HistoricoActivity"
            android:label="@string/title_activity_historico" >
        </activity>
    </application>

</manifest>
